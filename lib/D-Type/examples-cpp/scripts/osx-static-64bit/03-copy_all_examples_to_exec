##!/bin/csh

cd "$(dirname "$0")"

echo -------------------------------------------------------------------------------
echo Copy executables to the appropriate exec directory
echo -------------------------------------------------------------------------------

cp example_color_rasterizer ../../../exec/a-rasterizer/example_color_rasterizer_osx
cp example_basic_animation ../../../exec/b-foundation/example_basic_animation_osx
cp example_basic_output ../../../exec/b-foundation/example_basic_output_osx
cp example_font_converter ../../../exec/b-foundation/example_font_converter_osx
cp example_layout_extension ../../../exec/b-foundation/example_layout_extension_osx
cp example_powerdoc_output ../../../exec/c-powerdoc/example_powerdoc_output_osx
cp example_powerdoc_frames ../../../exec/c-powerdoc/example_powerdoc_frames_osx
cp example_powerdoc_viewer ../../../exec/c-powerdoc/example_powerdoc_viewer_osx
cp example_text_animation ../../../exec/d-text/example_text_animation_osx
cp example_text_output ../../../exec/d-text/example_text_output_osx
cp example_text_application ../../../exec/d-text/example_text_application_osx
cp example_textpro_builder1 ../../../exec/e-textpro/example_textpro_builder1_osx
cp example_textpro_builder2 ../../../exec/e-textpro/example_textpro_builder2_osx

echo -------------------------------------------------------------------------------
